{
	"name": "greenpeace/planet4-test-deimos",
	"description": "Greenpeace P4 test-deimos",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-main"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": null,
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/dc8b95967a6201a1370278353e444df2cfe373f6.zip"
				}
			}
		}
	],
	"scripts": {
		"core:language": "wp language core install pt_BR; wp language core activate pt_BR",
		"site:custom": [
			"@core:language"
		]
	}
}
